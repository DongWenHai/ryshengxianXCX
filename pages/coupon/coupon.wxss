.coupon-item{
  padding:10px 10px 0 10px;
  color:#fff;
}
.coupon-card{
  width:100%;
  height:320rpx;
  border-radius:8px;
  overflow: hidden;
  background:url(data:image/jpeg;base64,) no-repeat center;
  background-size:cover;
}
.coupon-info{
  width:100%;
  height:100%;
  padding:0 15px;
  box-sizing: border-box;
  position:relative;
}
.coupon-theme-danger{
    background:-moz-linear-gradient(120deg, rgba(244,112,104,0.92), rgba(239,94,78,0.92));
    background:-webkit-linear-gradient(120deg, rgba(244,112,104,0.92), rgba(239,94,78,0.92));
    background:linear-gradient(120deg, rgba(244,112,104,0.92), rgba(239,94,78,0.92));
}
.coupon-theme-assertive{
    background:-moz-linear-gradient(120deg, rgba(111,188,218,0.92), rgba(49,178,220,0.92));
    background:-webkit-linear-gradient(120deg, rgba(111,188,218,0.92), rgba(49,178,220,0.92));
    background:linear-gradient(120deg, rgba(111,188,218,0.92), rgba(49,178,220,0.92));
}
.coupon-theme-fresh{
    background:-moz-linear-gradient(120deg, rgba(104,234,157,0.92), rgba(55,208,100,0.92));
    background:-webkit-linear-gradient(120deg, rgba(104,234,157,0.92), rgba(55,208,100,0.92));
    background:linear-gradient(120deg, rgba(104,234,157,0.92), rgba(55,208,100,0.92));
}
.coupon-theme-warm{
    background:-moz-linear-gradient(120deg, rgba(255,222,140,0.92), rgba(255,198,59,0.92));
    background:-webkit-linear-gradient(120deg, rgba(255,222,140,0.92), rgba(255,198,59,0.92));
    background:linear-gradient(120deg, rgba(255,222,140,0.92), rgba(255,198,59,0.92));
}
.flex-box{
  display:-webkit-flex;
  display:flex;
  justify-content: space-between;
}
.coupon-name{
  font-size:38rpx;
  font-weight:bold;
  padding-top:40rpx;
}
.coupon-condition{
  font-size:30rpx;
  padding-top:10rpx;
  color:#f8f8f8;
}
.coupon-money-money{
  font-size:136rpx;
}
.coupon-money-type{
  font-size:56rpx;
}
.coupon-time{
  height:88rpx;
  line-height:88rpx;
  width:calc(100% - 30px);
  position:absolute;
  bottom:0;
  left:0;
  right:0;
  margin:0 auto;
  display:-webkit-flex;
  display:flex;
  justify-content: space-between;
  border-top:2px dashed #f8f8f8;
  font-size:28rpx;
  color:#f8f8f8;
}