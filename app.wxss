.search-box{
  padding:10px 15px;
  box-sizing: border-box;
  position:fixed;
  top:0;
  left:0;
  background:#fff;
  width:100%;
  z-index:99;
}
.search-input{
  width:100%;
  height:36px;
  background:#F3F1F2;
  border-radius:4px;
  position:relative;
  text-align: center;
  line-height:36px;
  color:#888;
  font-size:30rpx;
}
.search-icon{
  padding-top:4px;
}
.tabbar{
  width:100%;
  height:50px;
  background:#fff;
  position:fixed;
  bottom:0;
  left:0;
  z-index:99;
  box-shadow: 0 0 20rpx 0 #eee;
}
.cart-icon{
  width:20px;
  height:25px;
}
.cart-icon>image{
  width:20px;
  height:20px;
  padding-top:4px;
}
.list-img{
  width:180rpx;
  height:180rpx;
  overflow: hidden;
  border-radius:4px;
  background:#f3f3f3;
  position:relative;
}
.list-img>image{
  position:absolute;
  top:50%;
  left:50%;
  -webit-transform:translate(-50%,-50%);
  -o-transform:translate(-50%,-50%);
  -moz-transform:translate(-50%,-50%);
  -ms-transform:translate(-50%,-50%);
  transform:translate(-50%,-50%);
}
.list-content{
  width:calc(100% - 180rpx);
  padding-left:10rpx;
  box-sizing: border-box;
}
.noData{
  position:absolute;
  top:50%;
  left:50%;
  -webkit-transform: translate(-50%,-50%);
  transform: translate(-50%,-50%);
}
.noData>image{
  width:180rpx;
  margin:0 auto;
}
.noData>view{
  font-size:30rpx;
  color:#999;
  text-align: center;
}
.loading-icon{
  width:100%;
  text-align: center;
  margin-top:30rpx;
}
.loading-icon>image{
  width:48rpx;
  height:48rpx;
  display:inline-block;
}
.loading-icons{
  width:100%;
  height:44px;
  text-align: center;
  margin-top:30rpx;
}
.loading-icons>image{
  width:64rpx;
  display:inline-block;
}
.pageline,.pageline>image{
  width:100%;
}

.modelConfirm{
  float:right;
  color:#1AAD19;
  font-size:30rpx;
  line-height:64rpx;
  padding:0 20rpx;
  border-radius:8rpx;
}
.modelConfirm:active{
  background:rgba(0,0,0,.1);
}
button.modelConfirm{
  background:#fff;
}
button.modelConfirm::after{
  border:none;
}

.count-control{
  display:-webkit-flex;
  display:flex;
  justify-content: flex-end;
  align-items: center;
}
.count-c{
  min-width:25px;
  height:25px;
  display:inline-block;
  text-align: center;
  line-height:25px;
}
.c-plus{
  background-image:url(data:image/png;base64,);
  background-repeat: no-repeat;
  background-size:cover;
}
.c-reduce{
  background-image:url(data:image/png;base64,);
  background-repeat: no-repeat;
  background-size:cover;
}
.weui-btn{
    position: relative;
    display: block;
    margin-left: auto;
    margin-right: auto;
    padding-left: 14px;
    padding-right: 14px;
    box-sizing: border-box;
    font-size: 18px;
    text-align: center;
    text-decoration: none;
    color: #FFFFFF;
    line-height: 2.55555556;
    border-radius: 5px;
    -webkit-tap-highlight-color: rgba(0, 0, 0, 0);
    overflow: hidden;
}
.hover{
  background:transparent;
  opacity: 0.7;
}

